      os="win32"
      ws="win32"
      arch="x86">
      <archiveFormat format="antZip"></archiveFormat>
    </platform>
    <platform
      os="linux"
      ws="gtk"
      arch="x86">
      <archiveFormat format="tar" />
    </platform>
    <platform
      os="macosx"
      ws="carbon"
      arch="x86">
